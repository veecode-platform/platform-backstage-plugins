{
  "name": "@veecode-platform/plugin-veecode-platform-module",
  "description": "Functionalities for the veecode-platform plugins",
  "version": "0.1.2",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "common-library",
    "pluginId": "veecode-platform",
    "pluginPackages": [
      "@veecode-platform/plugin-veecode-platform-common",
      "@veecode-platform/plugin-veecode-platform-module"
    ]
  },
  "sideEffects": false,
  "scripts": {
    "build": "backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack"
  },
  "devDependencies": {
    "@backstage/cli": "^0.29.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@backstage/backend-plugin-api": "^1.1.1",
    "@backstage/plugin-catalog-node": "^1.15.1"
  }
}
